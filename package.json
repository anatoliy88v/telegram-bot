{
  "name": "telegraf",
  "version": "4.8.5",
  "description": "Modern Telegram Bot Framework",
  "license": "MIT",
  "author": "Vitaly Domnikov <oss@vitaly.codes>",
  "homepage": "https://telegraf.js.org",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/telegraf/telegraf.git"
  },
  "bugs": {
    "url": "https://github.com/telegraf/telegraf/issues"
  },
  "main": "lib/index.js",
  "exports": {
    ".": {
      "types": "./typings/index.d.ts",
      "require": "./lib/index.js",
      "import": "./lib/index.js"
    },
    "./types": {
      "types": "./types.d.ts",
      "require": "./types.js",
      "import": "./types.js"
    }
  },
  "files": [
    "bin/*",
    "src/**/*.ts",
    "lib/**/*.js",
    "typings/**/*.d.ts",
    "typings/**/*.d.ts.map",
    "types.*"
  ],
  "bin": {
    "telegraf": "bin/telegraf"
  },
  "scripts": {
    "prepare": "npm run --silent build",
    "build": "tsc --build docs/examples",
    "build:docs": "typedoc src/index.ts",
    "pretest": "npm run build",
    "test": "ava test/*",
    "lint": "eslint .",
    "checks": "npm test && npm run lint",
    "refresh": "npm run clean && npm ci",
    "clean": "git clean -fX .eslintcache docs/build/ lib/ typings/",
    "start": "nodemon main.js"
  },
  "type": "commonjs",
  "engines": {
    "node": "^12.20.0 || >=14.13.1"
  },
  "types": "./typings/index.d.ts",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "jimp": "^0.16.2",
    "minimist": "^1.2.7",
    "module-alias": "^2.2.2",
    "node-fetch": "^2.6.7",
    "openai": "^3.1.0",
    "p-timeout": "^4.1.0",
    "pexels": "^1.4.0",
    "safe-compare": "^1.1.4",
    "sandwich-stream": "^2.0.2",
    "typegram": "^3.12.0",
    "uuid": "^8.3.2",
    "wikifakt": "^1.0.3"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^17.0.45",
    "@types/node-fetch": "^2.6.2",
    "@types/safe-compare": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "ava": "^4.3.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "typedoc": "^0.22.18",
    "typescript": "^4.8.4"
  },
  "keywords": [
    "telegraf",
    "telegram",
    "telegram bot api",
    "bot",
    "botapi",
    "bot framework"
  ]
}
